<!--setup html for the genre page-->
<!--setup expressions to display genre catagories and images-->
<!--setup #each helper to generate new genre title/link for each genre object in the database-->
<button id="logout" type="text">Logout</button>
<div class="inventoryColumn">
<h2 class="loginTitle">Genres</h2>
<div class="orderGenre">
    {{#each genre}}
    <a href="/genre/{{id}}/albums">
    <div class="orderImage">
        <h3>{{genre_name}}</h3>
        <img class="genreImage" src="./images/{{img}}" alt="light bulb"/>
    </div>
    </a>
    {{/each}}
</div>
  <script src="/js/logout.js"></script>
